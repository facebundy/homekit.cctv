{
  "UploadUrl": "http://localhost:5278/cctv/",
  "RabbitMQ": {
    "Uri": "amqp://guest:guest@localhost:5672/",
    "Exchange": "homekit.cctv",
    "Queue": "homekit.cctv.metadata",
    "LogQueue": "homekit.cctv.metadata.log",
    "LogRoutingKey": "log",
    "RoutingKey": "drive-info"
  },
  "UploadInterval": 30,
  "Directory": {
    "RootUrl": "C:\\Program Files\\Agent\\Media\\WebServerRoot\\Media",
    "VideoUrl": "video",
    "AudioUrl": "audio",
    "AutoDeleteInterval": 5,
    "MaxSizeLimitMb": 4000  
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "EventLog": {
      "SourceName": "Homekit CCTV",
      "LogName": "Application",
      "LogLevel": {
        "Microsoft": "Information",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    }
  }
}
